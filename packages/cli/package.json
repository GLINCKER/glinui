{
  "name": "glinui",
  "version": "0.1.1",
  "description": "CLI for adding Glin UI liquid glass components to your project.",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GLINCKER/glinui",
    "directory": "packages/cli"
  },
  "homepage": "https://glinui.com",
  "bugs": "https://github.com/GLINCKER/glinui/issues",
  "keywords": [
    "glinui",
    "glassmorphism",
    "liquid-glass",
    "react",
    "tailwindcss",
    "radix-ui",
    "cli",
    "ui"
  ],
  "bin": {
    "glinui": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./registry": {
      "types": "./dist/registry/index.d.ts",
      "default": "./dist/registry/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:release": "pnpm build && pnpm publish --access public"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "prompts": "^2.4.2",
    "picocolors": "^1.1.1",
    "fs-extra": "^11.3.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.5",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  }
}
